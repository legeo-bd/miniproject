const int mq6Pin = A5;
int sensorValue = 0;

void setup() {
  Serial.begin(9600);
  Serial.println("Warming up...");
  delay(20000);
}

void loop()
{
  sensorValue = analogRead(mq6Pin);
  Serial.print("Gas Concentration: ");
  Serial.println(sensorValue);

  if (sensorValue > 400)
  {
    Serial.println("Warning: Gas leakage detected!");
  }

  delay(1000);
}
